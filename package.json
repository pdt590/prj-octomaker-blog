{
  "name": "octomaker-blog",
  "version": "1.0.0",
  "description": "Blog website for octomaker project",
  "author": "pdthang",
  "private": true,
  "scripts": {
    "dev": "cross-env PORT=3000 nuxt",
    "build": "cross-env NODE_ENV=production BASE_URL=https://blog.octomaker.com nuxt build",
    "start": "cross-env NODE_ENV=production PORT=3000 nuxt start",
    "generate": "nuxt generate",
    "build:firebase": "yarn clean && yarn build && yarn copy && cd \"functions\" && yarn",
    "clean": "yarn clean:public && yarn clean:functions && yarn clean:static",
    "clean:functions": "rimraf \"functions/node_modules\" && rimraf \"functions/.nuxt\"",
    "clean:public": "rimraf \"public/**/*.*!(md)\" && rimraf \"public/_nuxt\"",
    "clean:static": "rimraf \"src/static/sw.js\"",
    "copy": "yarn copy:nuxt && yarn copy:static",
    "copy:nuxt": "xcopy \"functions\\.nuxt\\dist\\*\" \"public\\_nuxt\\\" /E /Y",
    "copy:static": "xcopy \"src\\static\\*\" \"public\\\" /E /Y",
    "start:firebase": "firebase serve --only functions,hosting",
    "deploy": "firebase deploy --only functions,hosting"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@bazzite/nuxt-optimized-images": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@nuxtjs/component-cache": "^1.1.5",
    "buefy": "^0.8.10",
    "compressorjs": "^1.0.6",
    "cookieparser": "^0.1.0",
    "cross-env": "^7.0.0",
    "file-saver": "^2.0.2",
    "firebase": "^7.7.0",
    "firebase-admin": "^8.9.2",
    "github-markdown-css": "^3.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "jimp": "^0.9.3",
    "js-cookie": "^2.2.1",
    "lqip-loader": "^2.2.0",
    "nuxt": "^2.11.0",
    "nuxt-i18n": "^6.5.0",
    "prismjs": "^1.19.0",
    "responsive-loader": "^1.2.0",
    "simplemde": "^1.11.2",
    "sqip-loader": "^1.0.0",
    "tocbot": "^4.10.0",
    "vue-lazyload": "^1.3.3",
    "vuelidate": "^0.7.5",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.8.3",
    "connect-logger": "0.0.1",
    "core-js": "^2.6.11",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2"
  }
}
